{
  "name": "@mocks-server/admin-api-client-data-provider",
  "version": "2.0.3",
  "description": "Client of @mocks-server/plugin-admin-api built with @data-provider",
  "keywords": [
    "mocks-server-plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/admin-api-client-data-provider",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint *.js test src jest.config.js test-e2e/browser/react-app/src test-e2e/browser/cypress test-e2e/browser/vanilla-app/public/main.js test-e2e/nodejs/es6/test test-e2e/nodejs/es6/index.js --resolve-plugins-relative-to=.",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:coverage": "npm run test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test:e2e:install": "cd test-e2e/mocks && npm i && cd ../nodejs/es6 && npm i && cd ../../browser && npm i && npm run cypress:install && cd react-app && npm i && cd ../vanilla-app && npm i",
    "test:e2e:browser": "cd test-e2e/browser && npm run test:react-app && npm run test:vanilla-app",
    "test:e2e:nodejs": "cd test-e2e/nodejs/es6 && npm run test:ci",
    "test:e2e": "npm run test:e2e:browser && npm run test:e2e:nodejs",
    "test:e2e:ci": "npm run test:e2e:install && npm run test:e2e",
    "test:ci": "npm run build && npm run test:coverage && npm run test:e2e:ci"
  },
  "peerDependencies": {
    "@data-provider/axios": "^2.0.0"
  },
  "dependencies": {
    "@mocks-server/admin-api-paths": "1.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "7.10.3",
    "@rollup/plugin-babel": "5.0.4",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "redux": "4.0.5",
    "@data-provider/core": "2.5.2",
    "@data-provider/axios": "2.1.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.9",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "rollup": "2.18.0",
    "rollup-plugin-terser": "6.1.0",
    "sinon": "9.0.2"
  },
  "lint-staged": {
    "test/**/*.js": "eslint",
    "src/**/*.js": "eslint",
    "*.js": "eslint",
    "test-e2e/browser/react-app/src/**/*.js": "eslint",
    "test-e2e/browser/cypress/**/*.js": "eslint",
    "test-e2e/browser/vanilla-app/public/main.js": "eslint",
    "test-e2e/nodejs/es6/test/**/*.js": "eslint",
    "test-e2e/nodejs/es6/index.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x"
  }
}
